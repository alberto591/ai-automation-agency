{
    "builds": [
        {
            "src": "api.py",
            "use": "@vercel/python"
        },
        {
            "src": "landing_page/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/health",
            "dest": "api.py"
        },
        {
            "src": "/api/(.*)",
            "dest": "api.py"
        },
        {
            "src": "/webhooks/(.*)",
            "dest": "api.py"
        },
        {
            "src": "/(.*)",
            "dest": "/landing_page/$1"
        }
    ]
}